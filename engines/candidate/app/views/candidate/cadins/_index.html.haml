- @cadins.each do |a|
  %tr
    %td
      = a.name
    %td
      = a.cpf
    %td
      = a.process
    %td.col-md-2
      = link_to "", edit_cadin_path(a), title: "editar",  class: 'glyphicon glyphicon-share-alt btn btn-primary btn-xs',data: {disable_with: "carregando..."}
      = link_to "", cadin_path(a), title: "visualizar",  class: 'glyphicon glyphicon-eye-open btn btn-success btn-xs',data: {disable_with: "carregando..."}
      = link_to "", cadin_path(a), method: :delete, data: { confirm: 'Are you sure?' }, title: 'Deletar', class: 'glyphicon glyphicon-remove btn btn-danger btn-xs',data: {disable_with: "carregando..."}
