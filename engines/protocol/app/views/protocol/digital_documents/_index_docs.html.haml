- @digital_documents.each_with_index do |j, i|
  %tr
    %td.col-md-1
      = i + 1
    %td
      = j.page_number
    %td
      = link_to "Visualizar", j.doc_path_url, target: '_blank'
    %td.col-md-2
      = link_to "", assessment_digital_document_path(assessment_id:@assessment,id:j), method: :delete, remote:true, class: 'md-highlight-remove btn btn-danger waves-effect waves-button waves-float', data: {confirm: "#{t(:confirm_delete)}"}
