= simple_form_for [@project, @subscribe, @candidate] do |f|
  .row
    .col-md-4
      = f.input :name
    .col-md-2
      = f.input :cpf
    .col-md-2
      = f.input :cau_br
  .row
    .col-md-3
      = f.association :state, prompt: "selecione..."
    .col-md-3
      = f.input :city
    .col-md-2
      = f.input :cep
  .row
    .col-md-6
      = f.input :address
    .col-md-2
      = f.input :burgh
  .row
  %hr/
  .row
    .col-md-2
      = f.input :telephone
    .col-md-2
      = f.input :celphone
    .col-md-4
      = f.input :email
  %hr/
  .row
    .col-md-4
      = f.input :fantasy_name
    .col-md-4
      = f.input :social_reason
  .row
    .col-md-2
      = f.input :cnpj
  %hr/
  .row
    .col-md-2
      = f.input :terms_use
  %hr/
  .row
    .col-md-2
      = f.button :submit