- @cadastre.kins.each do |j, i|
 %tr
  %td
   = j.name
  %td
   = j.cpf
  %td.col-md-2
   = link_to "", edit_kin_path(j), class: 'md-mode-edit btn btn-primary waves-effect waves-button waves-float'
   = link_to "", kin_path(j), method: :delete, class: 'md-highlight-remove btn btn-danger waves-effect waves-button waves-float', data: {confirm: "#{t(:confirm_delete)}"}
