= simple_form_for([:extranet,@agenda,@schedule]) do |f|
  .card-body.card-padding
    .row
      .col-md-12
        = f.label :status, class: 'fg-label'
        = f.input :status, as: :select, label: false, collection: RegularizationSchedule::Schedule.statuses.keys.to_a, prompt: 'selecione...'
    .row
      .col-md-12
        = f.button :submit, class: 'btn btn-success'
        = link_to "voltar", extranet_agendas_path,class: 'btn btn-inverse'
