- @attendants.each_with_index do |j, i|
 %tr
  %td
   = j.staff.name if j.staff.present?
  %td
   = j.type_attendant_i18n
  %td.col-md-2
   = link_to "", edit_attendant_path(j), remote: true, class: 'md-mode-edit btn btn-primary waves-effect waves-button waves-float'
   = link_to "", attendant_path(j), method: :delete, class: 'md-highlight-remove btn btn-danger waves-effect waves-button waves-float', data: {confirm: "#{t(:confirm_delete)}"}





