.panel.panel-default
  .panel-heading
    .panel-title
      %h4
        Ramal
        = link_to 'novo ramal', new_branch_line_path,remote: true, class: 'pull-right'
  .panel-body
    .table-responsive{:style => "overflow: hidden; outline: none;", :tabindex => "3"}
      %table.table.table-hover.table-bordered
        %thead
          %tr
            %th #
            %th Nome
            %th Ramal
            %th Status
            %th Ações
        %tbody.index-branch_line
          = render 'index_branch'
