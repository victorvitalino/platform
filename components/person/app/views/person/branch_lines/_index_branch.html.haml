- @branch_lines.each_with_index do |j, i|
  %tr
    %td.col-md-1
      = i + 1
    %td
      = j.telephone
    %td
      = j.description
    %td
      = j.status ? "Ativo" : "Inativo"
    %td.col-md-2
      = link_to "", sector_branch_line_path(sector_id:@sector,id:j), method: :delete, remote:true, class: 'glyphicon glyphicon-remove btn-xs btn btn-danger', data: {confirm: "#{t(:confirm_delete)}"} if policy(:BranchLine, namespace: Person).destroy?
