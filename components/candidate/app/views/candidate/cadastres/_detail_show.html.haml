.row
  .col-md-12
    .panel.panel-default
      .panel-heading{style: 'margin-bottom: 0px;'}
        %h4.panel-title
          %a.no-color{"aria-controls" => "collapseExample", "aria-expanded" => "false", "data-toggle" => "collapse", :href => "#collapse_one", :role => "button"}
            Informações básicas
.row.collapse#collapse_one.in
  .col-md-12
    %table.table.table-striped.table-condensed.table-bordered
      %thead
        %tr
          %th{colspan: 3} Nome Completo
          %th CPF
          %th Nascimento
          %th Idade
          %th Renda
      %tbody
        %tr
          %td{colspan: 3}= cadastre.name
          %td= cadastre.cpf
          %td= cadastre.born.strftime('%d/%m/%Y') if cadastre.born.present?
          %td= "#{cadastre.age} ano(s)" if cadastre.born.present?
          %td= cadastre.income
      %thead
        %tr
          %th Sexo
          %th Estado Cívil
          %th Chegada ao DF
          %th Naturalidade
          %th Condição Especial
          %th CID
          %th NIS
      %tbody
        %tr
          %td.col-md-1= cadastre.gender
          %td.col-md-1= cadastre.civil_state.name if cadastre.civil_state.present?
          %td.col-md-1= cadastre.arrival_df.strftime('%d/%m/%Y') if cadastre.arrival_df.present?
          %td.col-md-1= "#{cadastre.place_birth} #{cadastre.born_uf}"
          %td.col-md-2= cadastre.special_condition.name if cadastre.special_condition.present?
          %td.col-md-1= cadastre.cid
          %td.col-md-1= cadastre.nis


.row
  .col-md-12
    .panel.panel-default
      .panel-heading{style: 'margin-bottom: 0px;'}
        %h4.panel-title
          %a.no-color{"aria-controls" => "collapseExample", "aria-expanded" => "false", "data-toggle" => "collapse", :href => "#collapse_two", :role => "button"}
            Informações de contato
.row.collapse#collapse_two
  .col-md-12
    %table.table.table-striped.table-condensed.table-bordered
      %thead
        %tr
          %th CEP
          %th Cidade
          %th{colspan: 2} Endereço
      %tbody
        %tr
          %td.col-md-1= cadastre.cep
          %td.col-md-2= "#{cadastre.city.name} / #{cadastre.city.state.acronym}" if cadastre.city.present? && cadastre.city.state.present?
          %td.col-md-4{colspan: 2}= cadastre.address
      %thead
        %tr
          %th Telefone
          %th Telefone Opcional
          %th Celular
          %th E-mail
      %tbody
        %tr
          %td.col-md-2= cadastre.telephone
          %td.col-md-2= cadastre.telephone_optional
          %td.col-md-1= cadastre.celphone
          %td.col-md-1= cadastre.email.to_s.downcase

.row
  .col-md-12
    .panel.panel-default
      .panel-heading{style: 'margin-bottom: 0px;'}
        %h4.panel-title
          %a.no-color{"aria-controls" => "collapseExample", "aria-expanded" => "false", "data-toggle" => "collapse", :href => "#collapse_three", :role => "button"}
            Conjugê
.row.collapse#collapse_three
  .col-md-12
    %table.table.table-striped.table-condensed.table-bordered
      %thead
        %tr
          %th Nome
          %th CPF
          %th Data de nascimento
          %th RG
          %th Renda
      - spouse = cadastre.dependents.where(kinship_id: 6).first
      -if spouse.present?
        %tbody
          %tr
            %td= spouse.name.upcase
            %td= spouse.cpf
            %td= spouse.born.strftime('%d/%m/%Y') if spouse.born.present?
            %td= spouse.rg
            %td= spouse.income
        %thead
          %tr
            %th Condição especial
            %th CID
            %th NIS
        %tbody
          %tr
            %td= spouse.special_condition.name
            %td= spouse.cid
            %td= spouse.nis
.row
  .col-md-12
    .panel.panel-default
      .panel-heading{style: 'margin-bottom: 0px;'}
        %h4.panel-title
          %a.no-color{"aria-controls" => "collapseExample", "aria-expanded" => "false", "data-toggle" => "collapse", :href => "#collapse_four", :role => "button"}
            Dependentes
.row.collapse#collapse_four
  .col-md-12
    %table.table.table-striped.table-condensed.table-bordered
      %thead
        %tr
          %th Nome
          %th CPF
          %th Data de nascimento
          %th Parentesco
          %th Renda
      %tbody
        - cadastre.dependents.where('kinship_id <> 6').each do |dependents|
          %tr
            %td= dependents.name.upcase
            %td= dependents.cpf
            %td= dependents.born.strftime('%d/%m/%Y') if dependents.born.present?
            %td= dependents.kinship.name if dependents.kinship.present?
            %td= dependents.income
