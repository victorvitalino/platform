          %th Situação do imóvel
          %th Data
      %tbody
        - cadastre.cadastre_address.each do |cadastre_address|
          %tr
            %td= cadastre_address.unit.city.name.upcase
            %td= cadastre_address.unit.complete_address
            %td= cadastre_address.unit.situation_unit.description
