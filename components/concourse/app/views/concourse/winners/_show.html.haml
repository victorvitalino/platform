      = link_to image_tag('/images/pdf_flat.png', width: '150'), "http://www.codhab.df.gov.br/#{@winner.participation.archive_three.url}", target: '_blank'
    .col-md-2
      = link_to image_tag('/images/pdf_flat.png', width: '150'), "http://www.codhab.df.gov.br/#{@winner.participation.archive_four.url}", target: '_blank'
