.panel.panel-default
  .panel-heading
    %h3.panel-title
      Atendimento - Preenchimento de Checklist
  .panel-body
    .row
      .col-md-12
        %h3 Checklist
    %hr/
    .row
      .col-md-12
        %table.table.table-striped.table-bordered.table-hover
          %thead
            %tr
              %th Item
              %th Autenticado
              %th Checado
          %tbody.checklist-index
            = render 'index'
    .row
      .col-md-12
        %h3 Verificações
    %hr/
    .row
      .col-md-12
        %table.table.table-striped.table-bordered.table-hover
          %thead
            %tr
              %th Item
              %th Autenticado
              %th Checado
          %tbody.checklist-index
    .row
      .col-md-12
        %h3 Parecer do Analista
    %hr/
    .row
      .col-md-12
        
    %hr/
    .row
      .col-md-12
        = link_to "Voltar para atualização de dependentes", convoke_cadastre_mirror_dependent_mirrors_path(cadastre_mirror_id: @cadastre_mirror.id), class: 'btn btn-lg btn-default pull-left'
