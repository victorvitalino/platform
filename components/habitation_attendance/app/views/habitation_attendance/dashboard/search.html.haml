.panel.panel-default
  .panel-heading
    %h3.panel-title 
      Atendimento habitacional 
  .panel-body
    .row
      .col-md-3{style: 'margin-top: 10px'}
        = simple_form_for url_for, method: :get do |f|
          .row
            .col-md-12
              %label CPF (Somente n√∫meros)
              = f.input :cpf, label: false, input_html: { name: 'cpf', value: params[:cpf], class: 'cpf'}
          .row
            .col-md-2
              = f.submit "Pesquisar CPF", name: nil, class: 'btn btn-success', data: { disable_with: t(:disable_with)}
          %hr/
      .col-md-8
        = render 'show'

    